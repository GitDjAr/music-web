{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "checkJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "noUnusedLocals": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/env.d.tsd.tssrc/env.d.tsenv.d.ts"],
  "references": [{
    "path": "./tsconfig.node.json"
  }]
}